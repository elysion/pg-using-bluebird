{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "env": {
    "amd": true,
    "browser": true,
    "mocha": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "block-scoped-var": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "callback-return": 2,
    /* "camelcase": 2, */
    "comma-dangle": 1,
    "consistent-return": 2,
    "curly": [2, "multi-line"],
    "dot-notation": 1,
    "eqeqeq": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-alert": 2,
    "no-caller": 2,
    "no-cond-assign": 2,
    "no-console": 1,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-else-return": 0,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    /* "no-invalid-this": 2, */
    "no-irregular-whitespace": [2, {"skipTemplates": false, "skipStrings": false}],
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-spaces": 1,
    "no-multiple-empty-lines": 1,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-param-reassign": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-sparse-arrays": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-useless-call": 2,
    "no-void": 2,
    "no-warning-comments": 1,
    "no-with": 2,
    "radix": 2,
    "semi": [2, "never"],
    "space-before-blocks": [2, "always"],
    "use-isnan": 2,
    "valid-typeof": 2,
    "wrap-iife": [2, "inside"]
  }
}
